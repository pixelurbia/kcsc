.streetcar {
	position: absolute;
	width:63%;
	overflow: hidden;
	top: 0px;
	right: 0;

	@include media ($mobile) {
		width:54%;
	}

	img {
		max-height: 440px;
		@include media ($mobile){
			max-height: 300px;
		}
	}
}

.streetcar.start {
	-webkit-animation: mymove 5s 1; /* Chrome, Safari, Opera */ 
    animation: mymove 5s 1;

    @include media ($mobile) {
    	-webkit-animation: mobilemove 5s 1; /* Chrome, Safari, Opera */ 
    animation: mobilemove 5s 1;
    }
}

@keyframes mymove {
    0%   {width:63%;}
    50% {width:270%; opacity:1; }
    51% {opacity:0; width:275%;}
    51.2% {opacity:0; width:5%;}
    52% {opacity:1;}
    100% {width:63%;}
}

@keyframes mobilemove {
    0%   {width:53%;}
    50% {width:500%; opacity:1; }
    51% {opacity:0; width:505%;}
    51.2% {opacity:0; width:5%;}
    52% {opacity:1;}
    100% {width:53%;}
}