.question {
	text-align: left;
	margin-right: 0px;
	padding:18px 0px;
	border-top:1px solid #e1e1e1;

	@include media ($mobile){
		width:100%;
	}
	&:nth-child(3n) {
		margin-right:0;
	}
	&:nth-child(6), &:nth-child(7) {
		border-bottom: 1px solid #e1e1e1;
	}


	p {
		color:$blue;
		@include main;
		font-size: 18px;

		&:before {
			content:'+';
			color:$blue;
			margin-right: 20px;
		}
	}
	.answer {
		display: none;

		p {
			display: inline-block;
			&:before {
				content:'';
			}
		}
	}
}


