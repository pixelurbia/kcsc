.sub-navi {
	display: inline-block;
	text-align: center;
	width:100%;
	padding:20px 0px;
	ul {
		display: inline-block;
		text-align: center;
		@include media ($tablet){
			padding: 0px;
			margin-bottom: 0px;
		}
		li {
			list-style: none;
			float: left;
			padding:0px 30px;
			border-right: 2px solid $blue;
			@include media ($mobile){
				padding:3px 10px;
			}

			&:last-child {
				border-right:0px;
			}
			.children {
				display: none;
				position: absolute;
				padding:20px;
				background:url(../images/top-tri.svg)no-repeat;
				background-size: 24px;
				background-position: top center;
				margin-left: -50px;


				li {
					padding:10px 30px;
					background:$blue;
					margin-top: -8px;

					a {
						color: white;
						&:hover {
							color:$navy;
						}
					}
				}
			}
		}
	}
}

.sub-navi ul li:hover > .children {
	display: block;
}

