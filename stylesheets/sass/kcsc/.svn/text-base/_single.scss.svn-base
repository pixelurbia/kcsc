.full {
	padding-bottom: 50px;
	img {
		width:100%;
	}

}

.content-flex:nth-child(2){
	padding-bottom: 0px;
}

.content-flex {
	width:100%;
	display: inline-block;
	text-align: center;
	padding-bottom: 100px;
	position: relative;
.centered {
	@include span-columns(4);
	display: inline-block;
	text-align: left;
	float: none;
	margin-top: 20px;

	@include media ($tablet){
		width:90%;
		margin-right: 0;
	}
	@include media ($mobile){
		width:80%;
	}
	p {
		text-align: justify;
		@include media ($mobile){
				text-align: left;
			}
		}
	}

	.content-half {
		width:44.849658%;
		margin-right: 2.829158%;
		display: inline-block;
		padding-left: 17.5%;
		text-align: left;
		float: left;

		@include media ($tablet) {
			padding-left: 5%;
		}

		@include media ($ipad){
			width:90%;
			float: none;
			text-align: center;
		}
		@include media ($mobile){
			width:80%;
			padding-left:0;
			float: none;
			margin-right: 0;
		}
		p {
			text-align: justify;

			@include media ($mobile){
				text-align: left;
			}
		}
	}
}


.image-flex {
	float: left;
	width:52.3%;
	display: inline-block;
	overflow: hidden;

	@include media ($ipad){
		padding-left: 5%;
	}
	@include media ($mobile){
		width:100%;
		padding:0;
	}
	img {
		width:100%;
	}
}


.other-h {
	&:nth-child(odd) {

		.content-half {
			float:right;
			padding-right: 17.5%;
			padding-left: 0;
			margin-left: 2.829158%;
			margin-right: 0;

			@include media ($ipad){
				float: none;
				padding-right: 5%;
			}

			@include media ($mobile){
				padding-right: 0;
					float: none;
			margin-right: 0;
			}
		}
	}
}

.faqs {

	padding:50px 0px;

	.question {

		h2 {
			&:before {
			content:'+';
			color:$blue;
			margin-right: 20px;
			@include main;
			font-size: 30px;
		}
	}
	.answer {
		p {

			@include para;
			color:$type;
			&:before {
				content:'';
				margin-right: 0px;

			}
		}
	}
	}
}

.question.full {
	padding:25px 0px;

}

.question.minus {
	h2 {
		&:before {
			content:'-';
		}
	}
}

.logo-c {
	width:21%;
	margin-right: 4%;
	float: left;
	display: inline-block;
	text-align: center;
	padding:40px 0px 70px 0px;

	@include media ($mobile){
		width:50%;
		padding:30px 0px;
	}

	img {		
		max-height: 100px;
		max-width: 100%;
	@include media ($mobile){
		max-height: 70px;
	}

	}
}

